\n    }\n    return answers;\n}\nvoid Solve(){\n\tmemset(cnt, 0, sizeof(cnt));\n\tmemset(t, 0, sizeof(t));\n\tint n, q;\n\tcin\u0026gt;\u0026gt;n\u0026gt;\u0026gt;q;\n\tvector\u0026lt;int\u0026gt; tmp(n);\n\tmap\u0026lt;int, int\u0026gt; myMap;\n\tint cur = 1;\n\tfor(int i = 0; i \u0026lt; n; i++){\n\t\tcin\u0026gt;\u0026gt;tmp[i];\n\t\tif(!myMap.count(tmp[i])){\n\t\t\tmyMap[tmp[i]] = cur++;\n\t\t}\n\t}\n\tfor(int i = 0; i \u0026lt; n; i++){\n\t\tarr[i] = myMap[tmp[i]];\n\t\tid[arr[i]] = tmp[i];\n\t}\n\tblock_size = sqrt(n) + 1;\n\n\tvector\u0026lt;Query\u0026gt; v(q);\n\tfor(int i = 0; i \u0026lt; q; i++){\n\t\tcin\u0026gt;\u0026gt;v[i].l\u0026gt;\u0026gt;v[i].r;\n\t\tv[i].l--; v[i].r--;\n\t\tv[i].idx = i;\n\t}\n\tvector\u0026lt;ll\u0026gt; res = mo_s_algorithm(v);\n\tfor(auto x: res){\n\t\tcout\u0026lt;\u0026lt;x\u0026lt;\u0026lt;nl;\n\t}\n\n}\nint main(){\n\tios_base::sync_with_stdio(0);\n\tcin.tie(0); cout.tie(0);\n\tSolve();\n}\n\u003c/code\u003e\u003c/pre\u003e\u003c/div\u003e\u003c/div\u003e\u003cp\u003eNormal segment tree would not pass the time limit so I used \u003ca href=\"https://codeforces.com/blog/entry/18051\"\u003eefficient segment tree\u003c/a\u003e\u003c/p\u003e\u003c/div\u003e",
